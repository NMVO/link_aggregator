<h1>Link Finder</h1>
<% @links.each do |link| %>
  <div>
	<h1><%= link_to link.title, link.url %></h1> 
	<%= link.votes %>
	<%= link_to "Upvote", upvote_path(link), method: :put %>
	<%= link_to "Downvote", downvote_path(link), method: :put %>
	<%= link_to "Inputs", link_path(link) %>
	</div>
<% end %>


